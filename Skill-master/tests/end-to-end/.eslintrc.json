{
  "extends": [
    "eslint-config-airbnb-base"
  ],
  "globals": {
    "afterAll": true,
    "beforeAll": true,
    "browser": true,
    "by": true,
    "describe": true,
    "element": true,
    "expect": true,
    "it": true,
    "xit": true,
    "jasmine": true,
    "jQuery": true,
    "protractor": true,
    "sap": true
  },
  "rules": {
    "class-methods-use-this": "off",
    "func-names": "off",
    "import/no-dynamic-require": "off",
    "import/no-extraneous-dependencies": "off",
    "max-len": "off",
    "no-await-in-loop": "off",
    "no-console": "off",
    "no-param-reassign": "off",
    "no-restricted-syntax": "off",
    "no-unused-vars": [
      "error",
      {
        "args": "none"
      }
    ],
    "no-use-before-define": "off",
    "valid-jsdoc": "error"
  }
}