<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
>
	<P13nDialog
		id="dialog"
		showReset="true"
		showResetEnabled="true"
		ok="onOk"
		cancel="onCancel"
		reset="onReset"
		contentWidth="550px"
		contentHeight="450px"
	>
		<panels>
			<P13nColumnsPanel
				items="{
					path: '/Items'
				}"
				columnsItems="{
					path: '/ColumnsItems'
				}"
			>
				<items>
					<P13nItem
						columnKey="{columnKey}"
						text="{text}"
					/>
				</items>
				<columnsItems>
					<P13nColumnsItem
						columnKey="{columnKey}"
						index="{index}"
						visible="{visible}"
					/>
				</columnsItems>
			</P13nColumnsPanel>
		</panels>
	</P13nDialog>
</core:FragmentDefinition>