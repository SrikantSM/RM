{
  "parser": "@typescript-eslint/parser",
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "airbnb-typescript/base"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "import/prefer-default-export": "off",
    "class-methods-use-this": "off",
    "max-len": "off",
    "no-empty-function": "off",
    "no-useless-constructor": "off",
    "no-console": "off",
    "no-restricted-syntax": "off"
  }
}
