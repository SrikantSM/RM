{
	"name": "ui-integration",
	"private": true,
	"dependencies": {
		"concurrently": "^5.0.0",
		"test-commons": "git+https://github.tools.sap/Cloud4RM/Test-Commons#592e053d216af8140a5292be311615f9f14a88dc",
		"fs-extra": "^8.1.0",
		"grunt": "1.6.1",
		"grunt-cli": "1.4.3",
		"grunt-karma": "^4.0.2",
		"karma": "^6.3.4",
		"karma-chrome-launcher": "^3.1.0",
		"karma-coverage": "^2.2.1",
		"karma-html-reporter": "^0.2.7",
		"karma-junit-reporter": "^2.0.1",
		"karma-qunit": "^4.1.2",
		"karma-ui5": "^2.3.4",
		"qunit": "^2.17.2"
	},
	"devDependencies": {
		"eslint": "7.32.0",
		"eslint-config-airbnb-base": "14.2.1",
		"eslint-plugin-import": "2.24.1"
	},
	"scripts": {
		"lint": "eslint \"**/*.js\"",
		"prepare-umbrella": "cd ../../../ && npm ci",
		"prepare-server": "cd ../../ && npm ci && npm run build",
		"start": "npm run write && npm run prepare-umbrella && npm run prepare-server && concurrently --success first --kill-others \"npm run start-srv-server\" \"npm run start-integration-srv-server\" \"npm run all\"",
		"start2": "npm run write && npm run prepare-umbrella && npm run prepare-server && concurrently --success first --kill-others \"npm run start-srv-server\" \"npm run projectTeamMember\"",
		"start-srv-server": "cd ../../srv && mvn -s ../../settings.xml clean spring-boot:run -U -Dspring-boot.run.jvmArguments=\"-Dspring.profiles.active=local-test -Dmaven.test.skip=true\"",
		"start-integration-srv-server": "cd ../../integration-srv && mvn -s ../../settings.xml clean spring-boot:run -U -Dspring-boot.run.jvmArguments=\"-Dspring.profiles.active=local-test -Dmaven.test.skip=true\"",
		"write": "node write.js",
		"test": "npm start",
		"projectTeamMember": "npm run myProjectExperienceUi  && npm run myAssignmentsUi",
		"all": "npm run myResourcesUi && npm run projectRoleUi && npm run businessServiceOrgUi && npm run businessServiceOrgUpload && npm run availabilityUploadUi && npm run availabilityUpload && npm run availabilityDownload && npm run replicationScheduleUi",
		"myProjectExperienceUi": "grunt opa_tests --appName=myProjectExperienceUi --testCompiled=true",
		"debug-myProjectExperienceUi": "grunt opa_tests --appName=myProjectExperienceUi --debugMode=true --testCompiled=false",
		"projectRoleUi": "grunt opa_tests --appName=projectRoleUi --testCompiled=true",
		"debug-projectRoleUi": "grunt opa_tests --appName=projectRoleUi --debugMode=true --testCompiled=false",
		"businessServiceOrgUi": "grunt opa_tests --appName=businessServiceOrgUi --testCompiled=true",
		"debug-businessServiceOrgUi": "grunt opa_tests --appName=businessServiceOrgUi --debugMode=true --testCompiled=false",
		"businessServiceOrgUpload": "grunt opa_tests --appName=businessServiceOrgUpload --testCompiled=true",
		"debug-businessServiceOrgUpload": "grunt opa_tests --appName=businessServiceOrgUpload --debugMode=true --testCompiled=false",
		"availabilityUpload": "grunt opa_tests --appName=availabilityUpload --testCompiled=true",
		"debug-availabilityUpload": "grunt opa_tests --appName=availabilityUpload --debugMode=true --testCompiled=false",
		"availabilityUploadUi": "grunt opa_tests --appName=availabilityUploadUi --testCompiled=true",
		"debug-availabilityUploadUi": "grunt opa_tests --appName=availabilityUploadUi --debugMode=true --testCompiled=false",
		"availabilityDownload": "grunt opa_tests --appName=availabilityDownload --testCompiled=true",
		"debug-availabilityDownload": "grunt opa_tests --appName=availabilityDownload --debugMode=true --testCompiled=false",
		"replicationScheduleUi": "grunt opa_tests --appName=replicationScheduleUi --testCompiled=true",
		"debug-replicationScheduleUi": "grunt opa_tests --appName=replicationScheduleUi --debugMode=true --testCompiled=false",
		"myResourcesUi": "grunt opa_tests --appName=myResourcesUi --testCompiled=true",
		"debug-myResourcesUi": "grunt opa_tests --appName=myResourcesUi --debugMode=true --testCompiled=true",
		"myAssignmentsUi": "grunt opa_tests --appName=myAssignmentsUi --testCompiled=true",
		"debug-myAssignmentsUi": "grunt opa_tests --appName=myAssignmentsUi --debugMode=true --testCompiled=false"
	}
}
