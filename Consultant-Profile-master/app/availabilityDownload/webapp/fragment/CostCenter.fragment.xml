<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:cd="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<SelectDialog
		title="{i18n>costCenter}"
		items="{availability>/AvailabilityCostCenter}"
		search=".onValueHelpDialogSearch"
		confirm=".onValueHelpDialogClose($event,'costCenter')"
		cancel=".onValueHelpDialogClose"
		cd:FilterName="s4CostCenterId">
		<StandardListItem	
			title="{availability>s4CostCenterId}">
		</StandardListItem>
	</SelectDialog>
</core:FragmentDefinition>
